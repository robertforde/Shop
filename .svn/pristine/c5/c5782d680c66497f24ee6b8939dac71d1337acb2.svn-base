package com.daniel.model;

import java.util.Date;

/**
 * This class represents the order lines that are being refunded
 * @author Robert Forde
 *
 */
public class RefundedDetail {

	private int receiptNo;
	private int lineNo;
	private Date refundDate;
	private String saleType;
	private String itemCode;
	private String itemDescription;
	private int qty;
	private float price;
	private float discValue;
	private float valueExVat;
	private float vat;
	private float valueIncVat;
	private float costPrice;
	
	
	public int getReceiptNo() {
		return receiptNo;
	}
	
	public void setReceiptNo(int receiptNo) {
		this.receiptNo = receiptNo;
	}
	
	public int getLineNo() {
		return lineNo;
	}
	
	public void setLineNo(int lineNo) {
		this.lineNo = lineNo;
	}
	
	public Date getRefundDate() {
		return refundDate;
	}

	public void setRefundDate(Date refundDate) {
		this.refundDate = refundDate;
	}

	public String getSaleType() {
		return saleType;
	}

	public void setSaleType(String saleType) {
		this.saleType = saleType;
	}

	public String getItemCode() {
		return itemCode;
	}
	
	public void setItemCode(String itemCode) {
		this.itemCode = itemCode;
	}
	
	public String getItemDescription() {
		return itemDescription;
	}
	
	public void setItemDescription(String itemDescription) {
		this.itemDescription = itemDescription;
	}
	
	public int getQty() {
		return qty;
	}
	
	public void setQty(int qty) {
		this.qty = qty;
	}
	
	public float getPrice() {
		return price;
	}
	
	public void setPrice(float price) {
		this.price = price;
	}
	
	public float getDiscValue() {
		return discValue;
	}
	
	public void setDiscValue(float discValue) {
		this.discValue = discValue;
	}
	
	public float getValueExVat() {
		return valueExVat;
	}
	
	public void setValueExVat(float valueExVat) {
		this.valueExVat = valueExVat;
	}
	
	public float getVat() {
		return vat;
	}
	
	public void setVat(float vat) {
		this.vat = vat;
	}
	
	public float getValueIncVat() {
		return valueIncVat;
	}
	
	public void setValueIncVat(float valueIncVat) {
		this.valueIncVat = valueIncVat;
	}
	
	public float getCostPrice() {
		return costPrice;
	}
	
	public void setCostPrice(float costPrice) {
		this.costPrice = costPrice;
	}
	
}