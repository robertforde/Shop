/*Table structure for table `orderheader` */

DROP TABLE IF EXISTS `orderheader`;

CREATE TABLE `orderheader` (
  `receiptNo` int(11) NOT NULL AUTO_INCREMENT,
  `repNo` varchar(3) DEFAULT NULL,
  `totalExVat` decimal(10,2) DEFAULT '0.00' COMMENT 'Total (Ex Vat) (- Discount)',
  `totalVat` decimal(10,2) DEFAULT '0.00' COMMENT 'Total Vat Value',
  `totalPreRounding` decimal(10,2) NOT NULL DEFAULT '0.00' COMMENT 'totalExVat + totalVat',
  `rounding` decimal(10,2) DEFAULT '0.00' COMMENT 'rounding amount applied',
  `totalPostRounding` decimal(10,2) DEFAULT '0.00' COMMENT 'Total Invoice Value',
  `orderDate` date DEFAULT NULL,
  `payType` varchar(13) DEFAULT NULL,
  `saleType` varchar(11) DEFAULT NULL,
  `totalCostPrice` decimal(10,2) DEFAULT '0.00' COMMENT 'Total Cost of lines on this order',
  `custId` int(11) DEFAULT NULL,
  `name` varchar(30) DEFAULT NULL,
  `addressLine1` varchar(50) DEFAULT NULL,
  `addressLine2` varchar(50) DEFAULT NULL,
  `phone` varchar(15) DEFAULT NULL,
  `refundValue` decimal(10,2) DEFAULT '0.00',
  PRIMARY KEY (`receiptNo`),
  UNIQUE KEY `receiptNo_UNIQUE` (`receiptNo`),
  KEY `custId` (`custId`)
